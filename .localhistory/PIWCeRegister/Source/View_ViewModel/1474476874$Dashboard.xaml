<UserControl x:Class="PIWCeRegister.Source.View_ViewModel.Dashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dashboard="clr-namespace:PIWCeRegister.Source.View_ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
   
    <DockPanel LastChildFill="True">
		<StackPanel DockPanel.Dock="Left" Margin="0,0,0,66" >
            <Border BorderBrush="White" BorderThickness="1,1,1,1">
                <TextBlock TextWrapping="Wrap" Text=" hello your are n the dashboard!!"/>
            </Border>
        </StackPanel>
        <StackPanel DockPanel.Dock="Right" Margin="0,0,0,66" >
            <Border BorderBrush="White" BorderThickness="1,1,1,1">
                <TextBlock TextWrapping="Wrap" Text=" hello your are n the dashboard!!"/>
            </Border>
        </StackPanel>
        <StackPanel  DockPanel.Dock="Bottom" HorizontalAlignment="Stretch"  >
            <Border BorderBrush="White" BorderThickness="1,1,1,1">
                       <TextBlock TextWrapping="Wrap" Text=" hello your are n the dashboard!!"/>
            </Border>
            
		</StackPanel>
		
    </DockPanel>
</UserControl>
